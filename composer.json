{
    "name": "php-solution/api-user-bundle",
    "type": "bundle",
    "description": "Symfony bundle for working with user in API projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Kalinin",
            "email": "kalinick@gmail.com"
        }
    ],
    "require": {
        "php": "~7.0",
        "doctrine/doctrine-bundle": "~1.6",
        "doctrine/orm": "~2.5",
        "php-solution/api-response-lib": "~0.1",
        "php-solution/doctrine-orm-extra-lib": "~0.3",
        "symfony/dependency-injection": "~3.3",
        "symfony/framework-bundle": "~3.3",
        "symfony/http-kernel": "~3.3",
        "symfony/http-foundation": "~3.3",
        "symfony/security": "~3.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0"
    },
    "suggest": {
    },
    "autoload":     {
        "psr-4": { "PhpSolution\\ApiUserBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
    }
}